/**/
.mango {
    display: flex;
    flex-wrap: wrap;
    justify-content: space-between;
}

.mangoItems {
    width: 263px;
    margin-bottom: 39px;
    position: relative;
}

.mangoItems:hover {
    box-shadow: 0px 5px 8px 0px rgba(0, 0, 0, 0.16);
}

.mangoItems__img {
    width: 100%;
    filter: brightness(100%);
    @include transition(all, $sec);
}

.mangoItems:hover .mangoItems__img {
    filter: brightness(40%);
}

.mangoItems__content {
    padding: 16px;
}

.mangoItems__name {
    font-size: 14px;
    line-height: 1.2;
    font-weight: 400;
    text-transform: uppercase;
    color: #222222;
}

.mango__p {
    font-size: 16px;
    line-height: 1.2;
    font-weight: 700;
    text-transform: uppercase;
    color: $sait_color;
    margin-top: 17px;
}

.mangoItems__add {
    font-size: 13px;
    font-weight: bold;
    color: #ffffff;
    height: 39px;
    width: 123px;
    border: 1px solid #ffffff;
    display: none;
    align-items: center;
    justify-content: center;
    position: absolute;
    top: 97px;
    left: 70px;
    @include transition(position, $sec);
    @include transition(box-shadow, $sec);
}

.mangoItems__add:hover:before {
    content: "";
    background: url(../../../assets/img/cartSmall.svg) no-repeat center;
    width: 23px;
    height: 22px;
    position: absolute;
    left: 10px;
    top: 7px;
}

.mangoItems:hover .mangoItems__add {
    display: flex;
}

.mangoItems__add:hover {
    box-shadow: 0 0 6px #fff;
    padding-left: 30px;
}

/**/
.Allprod {
    text-align: center;
}

.buttonAllprod {
    width: 220px;
    overflow: hidden;
    position: relative;
    font-size: 16px;
    line-height: 7px;
    font-weight: 700;
    letter-spacing: 0.025em;
    color: #ffffff;
    padding: 18px 24px 19px 24px;
    border: 1px solid $sait_color;
    @include transition(background-color, $sec);
    @include transition(color, $sec);

    .r {
        font-size: 15px;
        line-height: 54px;
        font-weight: 400;
        position: absolute;
        display: flex;
        align-items: center;
        justify-content: center;
        right: 0;
        top: 0;
        opacity: 0;
        height: 100%;
        width: 40px;
        transition: all 0.2s linear 0s;
    }
}

.buttonAllprod:hover {
    border: 1px solid $sait_color;
    background-color: #fff;
    color: $sait_color;
    text-indent: -20px;

    .r {
        text-indent: 0px;
        opacity: 1;
    }
}

.offer {
    display: flex;
    margin-top: 132px;
    background-color: #222224;
